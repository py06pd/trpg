<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
          <rule name="wwwroot-font">
              <match url="([\S]+[.](woff|ttf|json|svg|js|css|gif|jpg|jpeg|map))" />
              <action type="Rewrite" url="editor/{R:1}" />
          </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>